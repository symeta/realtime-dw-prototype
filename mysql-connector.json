{
  "name": "mysql-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "<rds instance endpoint>",
    "database.port": "3306",
    "database.user": "<username>",
    "database.password": "<password>",
    "database.server.name": "tutorial",
    "database.include.list": "<database name>",
    "database.history.kafka.bootstrap.servers": "<MSK bootstrap server endpoints>",
    "database.history.kafka.topic": "mysql-cdc-history"
  }
}
